#Import all Visualization Libraries

import pandas as pd
import numpy as np
import seaborn as sns
%matplotlib inline

df1=pd.read_csv("df1",index_col=0)
df1.head()

df2=pd.read_csv("df2")
df2.head()

df1["A"].hist(bins=100)
df1["A"].plot.hist()

df2.plot.area(alpha=0.5)

df2.plot.bar(stacked=True)

df1.plot.scatter(x="A",y="B",c="C")

df1.plot.scatter(x="A",y="B",s=df1.C*50)

df2.plot.box()

df2.plot.density()
